{
	"_version": "1.1.0",
	"sap.app": {
		"_version": "1.1.0",
		"id": "com.meui5ncrud.app",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.32.0"
		}
	},
	"sap.ui": {
		"_version": "1.1.0",
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_bluecrystal"
		]
	},
	"sap.ui5": {
		"_version": "1.1.0",
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.meui5ncrud.app.i18n.i18n"
				}
			},
	 	 	"transactions": {
				"uri": "http://192.168.178.38:3000/employees/",
				"type": "sap.ui.model.json.JSONModel"
				
	  		},
	  		"somPerMaand": {
				"uri": "http://192.168.178.38:3000/somPerMaand/",
				"type": "sap.ui.model.json.JSONModel"
				
	  		},
	  		"rekeningen":{
	  			"type": "sap.ui.model.json.JSONModel",
				"uri": "model/rekeningen.json"
	  		},
	  		"post":{
	  			"uri": "http://192.168.178.38:3000/post",
				"type": "sap.ui.model.json.JSONModel"
	  		}

		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"viewType": "XML",
				"routerClass": "sap.m.routing.Router",
				"viewPath": "com.meui5ncrud.app.view",
				"controlId": "appView",
				"controlAggregation": "pages",
				"transition": "slide",
				"async": true
			},
			"routes": [{
				"name": "LandingsPage",
				"pattern": "",
				"greedy": false,
				"target": [
					"LandingsPage"
				]
			}, {
				"name": "List",
				"pattern": "List/",
				"greedy": false,
				"target": [
					"List"
				]
			}, {
				"name": "JaarOpgave",
				"pattern": "JaarOpgave/",
				"greedy": false,
				"target": [
					"JaarOpgave"
				]
				}, {
				"name": "Import",
				"pattern": "ExcelImport/",
				"greedy": false,
				"target": [
					"Import"
				]
				}, {
				"name": "transactionDetail",
				"pattern": "List/{rekening}/{mutatie}/{month}",
				"greedy": false,
				"target": [
					"List"
				]
				}
			],
			"targets": {
				"LandingsPage": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "LandingsPage"
				},
				"List": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "List"
				},
				"JaarOpgave": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "JaarOpgave"
				},
				"Import": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "ExcelImport"
				}
			}
		}
	},
	"sap.platform.hcp": {
		"uri": "webapp",
		"_version": "1.1.0"
	}
}